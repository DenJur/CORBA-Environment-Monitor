<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
    <SplitPane dividerPositions="0.2505567928730512, 0.7505567928730512" focusTraversable="true" VBox.vgrow="ALWAYS">
        <ListView fx:id="regionalList" prefHeight="200.0" prefWidth="200.0" />
        <ListView fx:id="stationList" prefHeight="200.0" prefWidth="200.0" />
        <VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity">
            <Label alignment="CENTER" style="&#10;" text="Notifications" textAlignment="CENTER">
                <font>
                    <Font size="18.0" fx:id="x1" />
                </font>
                <textFill>
                    <Color blue="0.624" green="0.624" red="0.624" fx:id="x2" />
                </textFill>
            </Label>
         <TextField fx:id="nameInput" promptText="Name">
            <VBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
         </TextField>
            <TextField fx:id="emailInput" promptText="Email">
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </VBox.margin>
            </TextField>
            <Button fx:id="emailAddButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="Add">
                <VBox.margin>
                    <Insets bottom="10.0" top="10.0" />
                </VBox.margin>
            </Button>
            <ListView fx:id="emailList" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" VBox.vgrow="ALWAYS">
                <VBox.margin>
                    <Insets bottom="10.0" top="10.0" />
                </VBox.margin>
            </ListView>
        </VBox>
    </SplitPane>
</VBox>
